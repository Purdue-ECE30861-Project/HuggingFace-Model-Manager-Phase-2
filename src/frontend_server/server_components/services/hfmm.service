[Unit]
Description=uvicorn instance for the Huggingface Model Manager frontend
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/opt/hfmm
Environment="PATH=/opt/hfmm/.venv/bin"
Environment="BACKEND_URL=172.31.34.188"
ExecStart=/opt/hfmm/.venv/bin/uvicorn src.frontend_server.__main__:api_core --workers 4 --uds /opt/hfmm/hfmm.sock --proxy-headers

[Install]
WantedBy=multi-user.target